{
  "name": "sans-sel",
  "version": "1.0.0dev",
  "author": "BenoÃ®t Zugmeyer",
  "description": "Write your components styles in JavaScript, without selectors.",
  "repository": "https://github.com/BenoitZugmeyer/sans-sel",
  "devDependencies": {
    "babel-preset-es2015": "^6.6.0",
    "babel-preset-es2015-rollup": "^1.1.1",
    "babel-register": "^6.6.0",
    "eslint": "^2.2.0",
    "mocha": "^2.4.5",
    "rollup-plugin-babel": "^2.4.0",
    "should": "^8.2.2"
  },
  "scripts": {
    "prepublish": "npm run dist && npm run dev",
    "dist": "mkdir -p dist && rollup -c | uglifyjs -c -m -d '__DEV__=false' > dist/sansSel.min.js",
    "dev": "mkdir -p dist && rollup -c --intro 'var __DEV__ = true;' > dist/sansSel.js",
    "coverage": "NODE_ENV=test istanbul cover $(which _mocha)",
    "test": "NODE_ENV=test mocha --compilers js:babel-register --require test/setDev.js",
    "lint": "eslint ."
  },
  "main": "./dist/sansSel",
  "files": [
      "dist/sansSel.js",
      "dist/sansSel.min.js",
      "src/*"
  ]
}
